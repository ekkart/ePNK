@ARTICLE{Kna92,
  author = {Edgar Knapp},
  title = {Derivation of concurrent programs: two examples},
  journal = {Science of Programming},
  year = {1992},
  volume = {19},
  pages = {1--23},
  number = {1},
  month = oct,
  abstract = { { We give two examples of how concurrent programs can be derived
	from their specifications much like sequential ones. As a formal
	framework for our research we use the UNITY formalism, due to Chandy
	and Misra. Starting from an initial UNITY specification we proceed
	in a series of strengthening steps until the specification is restrictive
	enough to be translated directly into UNITY code. The programs we
	obtain this way may be further refined to increase their efficiency,
