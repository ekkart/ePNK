@INPROCEEDINGS{AbLa90,
  author = {Mart{\'{\i}}n Abadi and Leslie Lamport},
  title = {Composing Specifications},
  booktitle = {Stepwise Refinement of Distributed Systems. Models, Formalisms, Correctness},
  year = {1990},
  editor = {J.W. de Bakker and W.-P. de Roever},
  volume = {430},
  series = {LNCS},
  publisher = {Springer-Verlag},
  note = {previously: SRC Research Report 66, October 1990},
  abstract = { { A rigorous modular specification method requires a proof rule asserting
	that if each component behaves correctly in isoloation, then it behaves
	correctly in concert with other components. Such a rule is subtle
	if a component need behave correctly only when its environment does,
	since each component is part of the others' environments. We examine
	the precise distinction between a system and its environment, and
	provide the requiste proof rule when modules are specified with safety
	and liveness properties.}},
  annote = {Compositionality, rely/guarantee},
  crossref = {BRR90}
}

