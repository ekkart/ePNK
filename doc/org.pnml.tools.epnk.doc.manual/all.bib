@TECHREPORT{Lam90a,
  author = {Leslie Lamport},
  title = {A Temporal Logic of Actions},
  institution = {Digital Equipment Corporation, Systems Research Center},
  year = {1990},
  type = {SRC Research Report},
  number = {57},
  month = apr,
  abstract = { { In 1977, Pnueli introduced to computer scientists a temporal logic
	for reasoning about concurrent programs. His logic was simple and
	elegant, based on the single temporal modality ``forever'', but it
	was not expressive enough to completely describe programs. Since
	then, a plethora of more expressive logics have been proposed, all
	with additional temporal modalities such as ``next'', ``until'',
	and ``since''. Here, a temporal logic is introduced based only on
	Pnueli's original modality ``forever'', but with predicates (assertions
	about s single state) generalized to actions --- assertions about
	pairs of states. The logic has all the expressive power needed to
	describe and reason about concurrent programs. Much of the temporal
	reasoning required with other logics is replaced by nontemporal reasoning
	about actions.}}
}

